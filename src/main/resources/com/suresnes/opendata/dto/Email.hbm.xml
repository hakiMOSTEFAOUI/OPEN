<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.suresnes.opendata.dto.Email" table="Emails">
        <id name="email" type="string">
            <column name="Email" precision="5" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="destinataire" type="string">
            <column name="Destinataire" length="50" not-null="true" />
        </property>
        
    </class>
</hibernate-mapping>